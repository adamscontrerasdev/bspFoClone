{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "out/types/**/*.ts",
    "types/**/*.d.ts"
, "src/app/hooks/useFavorite.jsx", "src/app/beatsfhome/beatslist.jsx", "src/app/header/Fav.jsx", "src/app/beatsfhome/colorAdaptativeDiv.jsx", "src/app/beatsfhome/reproductor.tsx"  ],
  "exclude": [
    "node_modules"
  ]
}
